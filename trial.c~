#include <stdio.h>

int main (void) {
    int array[36] = {0};
    int scan;

    while (scanf("%d", &scan) == 1) {
        array[scan] = 1;
    }

    int i = 0;
    while (i < 36) {
        printf("%d ", array[i]);
        i++;
    }
    printf("\n");

    return 0;
}
</stdio.h>
